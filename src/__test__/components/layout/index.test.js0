/* eslint-disable no-undef */
import React from 'react'
import { mount } from 'enzyme'
import { create } from 'react-test-renderer'
import LayoutMobile from '../../../components/layout/index'

describe('<LayoutMobile />', () => {
  const layoutMobile = mount(<LayoutMobile />)

  test('Render component Layout', () => {
    expect(layoutMobile.length).toEqual(1)
  })
})

describe('LayoutMobile Snapshot', () => {
  test('checkout of view of Layout', () => {
    const layoutMobile = create(<LayoutMobile />)
    expect(layoutMobile.toJSON()).toMatchSnapshot()
  })
})
